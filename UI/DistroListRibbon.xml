<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
    <ribbon>
        <tabs>
            <tab idMso="TabMail">
            <group id="DlToolGroup"
                    label="DL Tool">
				
				<button id="dlToolButton" 
						enabled="true"
						visible="true" 
						onAction="DlToolButtonOnAction" 
						label="Open DL Tool"
						imageMso="MenuInvite"
						size="large"
						/>
				
				<button id="configureButton"
						enabled="true"
						visible="true"
						onAction="DlToolButtonOnAction"
						label="Configure DL Tool"
						imageMso="MoreControlsDialog"
						size="large"
						/>
            </group>
            </tab>
        </tabs>
    </ribbon>
</customUI>

